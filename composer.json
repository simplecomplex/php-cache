{
    "name": "simplecomplex/cache",
    "type": "library",
    "description": "File-based PSR-16 Simple Cache implementation; cache management; CLI interface.",
    "keywords": ["PSR-16", "simple cache", "PHP"],
    "homepage": "https://github.com/simplecomplex/php-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Friis Mathiasen",
            "email": "jacob.friis@simplecomplex.net"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-ctype": "*",
        "psr/simple-cache": "^1.0 || dev-develop",
        "simplecomplex/utils": "^1.8 || ^2.0 || dev-develop"
    },
    "suggest": {
        "simplecomplex/inspect": "CLI features do NOT work without Inspect. And great for logging; better variable dumps and traces."
    },
    "autoload": {
        "psr-4": {
            "SimpleComplex\\Cache\\": "src/"
        }
    }
}
